{
    "name": "Jokify",
    "version": "0.1.0",
    "description": "A chrome extension that generates random jokes.",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "./images/logo.png"
    },
    "icons": {
        "16": "./images/smile.png",
        "128": "./images/logo.png"
    },
    "permissions": ["activeTab"]
}